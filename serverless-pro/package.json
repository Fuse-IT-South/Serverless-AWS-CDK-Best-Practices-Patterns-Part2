{
  "name": "serverless-pro",
  "version": "0.1.0",
  "bin": {
    "serverless-pro": "bin/pipeline.js"
  },
  "scripts": {
    "synth": "cdk synth --all",
    "format": "npx prettier --write .",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prepare": "cd .. && husky install config/.husky",
    "precommit": "npm run synth && npm run test && npm run lint:fix && npm run format",
    "prepush": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "deploy:pipeline": "cdk deploy ServerlessPro --profile=whatsapptracking-cdk",
    "remove:pipeline": "cdk destroy ServerlessPro --profile=whatsapptracking-cdk",
    "deploy:dev": "cdk deploy ServerlessPro/Develop-PR-123/StatefulStack ServerlessPro/Develop-PR-123/StatelessStack --profile=whatsapptracking-cdk",
    "preremove:dev": "cdk synth --profile=whatsapptracking-cdk",
    "remove:dev": "cdk destroy ServerlessPro/Develop-PR-123/StatefulStack ServerlessPro/Develop-PR-123/StatelessStack --profile=whatsapptracking-cdk"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.15",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.62.0",
    "artillery": "^2.0.0-30",
    "artillery-plugin-ensure": "^1.1.1",
    "artillery-plugin-expect": "^2.2.1",
    "aws-cdk": "2.150.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.150.0",
    "aws-sdk": "2.150.0",
    "cdk-nag": "^2.28.171",
    "constructs": "^10.0.0",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  }
}
